{
  "name": "nuxt-resource-module",
  "version": "0.1.0",
  "main": "dist/module.js",
  "repository": "https://github.com/mya-ake/nuxt-resource-module.git",
  "author": "mya-ake <make.app.ts@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s clean compile mkdir:templates copy:templates",
    "compile": "tsc",
    "clean": "rimraf dist",
    "copy:templates": "cp templates/* dist/templates",
    "mkdir:templates": "mkdir dist/templates",
    "lint": "tslint --config tslint.json 'src/**/*.ts' 'tests/**/*.ts'",
    "lint:fix": "tslint --config tslint.json --fix 'src/**/*.ts' 'tests/**/*.ts'",
    "test:unit": "jest --config jest.config.js",
    "test:unit:watch": "jest --config jest.config.js --watch"
  },
  "devDependencies": {
    "@types/jest": "^23.3.7",
    "@types/lodash": "^4.14.117",
    "@types/node": "^10.12.0",
    "jest": "^23.6.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.14.3",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.6",
    "rollup-plugin-typescript": "^1.0.0",
    "ts-jest": "^23.10.4",
    "tslib": "^1.9.3",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-config-standard": "^8.0.1",
    "tslint-plugin-prettier": "^2.0.0",
    "typescript": "^3.1.3"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.3.3",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1"
  }
}
